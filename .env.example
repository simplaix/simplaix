# Database configuration for PostgreSQL
POSTGRES_DATABASE="your_database_name"
POSTGRES_HOST="your_host_url"
POSTGRES_PASSWORD="your_password"
POSTGRES_USER="your_username"

# Database connection URLs
POSTGRES_PRISMA_URL="your_prisma_url" # Connection URL with pgbouncer for Prisma
POSTGRES_URL="your_postgres_url" # Standard PostgreSQL connection URL
POSTGRES_URL_NON_POOLING="your_non_pooling_url" # Connection URL without connection pooling
POSTGRES_URL_NO_SSL="your_no_ssl_url" # Connection URL without SSL

# OpenAI API configuration
OPENAI_API_KEY="your_openai_api_key" # API key for OpenAI services

# Authentication configuration
AUTH_SECRET="your_auth_secret" # Secret key for authentication

# Google OAuth configuration
NEXT_PUBLIC_GOOGLE_CLIENT_ID="your_google_client_id" # Google OAuth client ID
NEXT_PUBLIC_GOOGLE_API_KEY="your_google_api_key" # Google API key
NEXT_PUBLIC_REDIRECT_URI="your_redirect_uri" # OAuth redirect URI

# Vercel Blob storage configuration
BLOB_READ_WRITE_TOKEN="your_blob_token" # Token for Vercel Blob storage access

# Anthropic API configuration
ANTHROPIC_API_KEY="your_anthropic_api_key" # API key for Anthropic services